<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean class="com.he.spring.he3.bean.Field1" id="field1" p:name="str1"/>

    <!--List, Set, Map, å’Œ Properties-->
    <bean class="com.he.spring.he3.bean.BaseBean" id="baseBean">
        <property name="field1" ref="field1"/>
        <property name="list">
            <list>
                <value>list1</value>
                <value>1</value>
                <ref bean="field1"/>
            </list>
        </property>
        <property name="set">
            <set>
                <value>set1</value>
                <value>1</value>
                <ref bean="field1"/>
            </set>
        </property>
        <property name="map">
            <map>
                <entry key="mapKey1" value="mapVal1"/>
                <entry key-ref="field1" value-ref="field1"/>
            </map>
        </property>
        <property name="properties">
            <props>
               <prop key="propKey1" >propVal1</prop>
               <prop key="propKey2" >2</prop>
               <prop key="propKey3" >true</prop>
            </props>
        </property>
    </bean>


</beans>